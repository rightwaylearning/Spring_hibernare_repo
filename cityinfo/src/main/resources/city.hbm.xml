<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
  <hibernate-mapping>
     <class name="j1.entity.cityinfo.City"  table="city">
       <id name="cityCode" column="city_code"></id>
       <property name="cityName" column="city_name"></property>
       <property name="cityState" column="city_state"></property>
       <property name="cityPin" column="city_pin"></property>
     </class>
  
  </hibernate-mapping>  