<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
  <hibernate-mapping>
     <class name="model.classes.Product"  table="product">
       <id name="productId" column="product_id"></id>
       <property name="productName" column="product_name"></property>
       <property name="productPrice" column="product_price"></property>
       <property name="productType" column="product_type"></property>
       <property name="productBrand" column="product_brand"></property>
     </class>
    <class name="model.classes.Temp"  table="temp">
       <id name="pId" column="pid"></id>
       <property name="pName" column="pname"></property>
       <property name="pPrice" column="pprice"></property>
       <property name="pType" column="ptype"></property>
       <property name="pBrand" column="pbrand"></property>
     </class>
  
  </hibernate-mapping>  